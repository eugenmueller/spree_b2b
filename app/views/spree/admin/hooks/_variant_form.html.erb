<div data-hook="admin_variant_form_dealer_price">
  <%= f.field_container :dealer_price, class: ['form-group'] do %>
    <%= f.label :dealer_price, raw(Spree.t(:dealer_price) + content_tag(:span, ' *', class: "required")) %>
    <%= f.text_field :dealer_price, value: number_to_currency(@variant.dealer_price, precision: 2), class: 'form-control'  %>
    <%= f.error_message_on :dealer_price %>
  <% end %>
</div>
